            <a href="/Website//index.html" class="nav__link active">Home</a>
          </div>

          <div class="row mb-3">
            <!-- Sugnlight -->
            <div class="col-xl-3 col-md-6 mb-4">
              <div class="card h-100">
                <div class="card-body">
                  <div class="row align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-uppercase mb-1">Sunlight</div>
                      <div class="text-xs font-weight-bold text-gray-700 mb-1">Min:<%=topPlant.min_light_lux  %>, Max: <%=topPlant.max_light_lux  %></div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800"><%=(topPlant.max_light_lux+topPlant.min_light_lux)/2  %></div>
                      <div class="mt-2 mb-0 text-muted text-xs">
                        <span>Measured in lumons</span>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-sun fa-2x text-primary"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Moisture -->
            <div class="col-xl-3 col-md-6 mb-4">
              <div class="card h-100">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-uppercase mb-1">Moisture</div>
                      <div class="text-xs font-weight-bold text-gray-700 mb-1">Min:<%=topPlant.min_env_humid  %>, Max: <%=topPlant.max_env_humid  %></div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800"><%=(topPlant.max_env_humid+topPlant.min_env_humid)/2  %></div>
                      <div class="mt-2 mb-0 text-muted text-xs">
                        <span title="units of grams of water vapour per cubic metre of air.">Measured in g.m-3</span>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-water fa-2x text-success"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Nutrients -->
            <div class="col-xl-3 col-md-6 mb-4">
              <div class="card h-100">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-uppercase mb-1">Soil Moisture</div>
                      <div class="text-xs font-weight-bold text-gray-700 mb-1">Min:<%=topPlant.min_soil_moist  %>, Max: <%=topPlant.max_soil_moist %></div>
                      <div class="h5 mb-0 mr-3 font-weight-bold text-gray-800"><%=(topPlant.min_soil_moist+topPlant.max_soil_moist)/2 %></div>
                      <div class="mt-2 mb-0 text-muted text-xs">
                        <span>Measured in kilopascals</span>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-leaf fa-2x text-info"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Temperature -->
            <div class="col-xl-3 col-md-6 mb-4">
              <div class="card h-100">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-uppercase mb-1">Temperature</div>
                      <div class="text-xs font-weight-bold text-gray-700 mb-1">Min:<%=topPlant.min_temp  %>, Max: <%=topPlant.max_temp%></div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800"><%=(topPlant.max_temp+topPlant.min_temp)/2  %></div>
                      <div class="mt-2 mb-0 text-muted text-xs">
                        <span>Measured in degrees celsius</span>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-temperature-high fa-2x text-warning"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Graph -->
            <div class="col-xl-8 col-lg-7 mb-4"  >
              <div class="card mb-4" style="height: 100% !important;">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Plant Information: <%=topPlant.name%></h6>                  
                </div>
                
                <p class="TP">Scientific name: <%=topPlant.scientific_name%></p>
                <p class="Plant"><%=topPlant.description%></p>
              
              </div>
            </div>
            <!-- Pie Chart -->
            <div class="col-xl-4 col-lg-4 mb-4">
              <div class="card">
                <img  src="<%=topPlant.url%>" class="card-img-top" alt="<%=topPlant.name  %> ">
                <div class="card-body">
                  <p class="card-text"><%=topPlant.name  %></p>
                </div>
              </div>
            </div>

            <!-- Plant Detenction -->
            <div class="col-xl-8 col-lg-7 mb-4">
              <div class="card ">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Supported Plants</h6>
                </div>
                  <div style="overflow-y: auto; height: 400px"class="table-responsive">
                    <table class="table table-light table-hover ">
                      <thead>
                      <tr>
                        <th class="header" scope="col">Common name</th>
                        <th class="header" scope="col">Family Category</th>
                        <th class="header" scope="col"></th>
                      </tr>
                    </thead>
                    <tbody>
                      <%for(var i = 0; i<allplants.length; i++) {%> 
                      <tr>
                        <td scope="row"><a title="Click to learn more about <%=allplants[i].name %>" href="/Dashboard/<%=allplants[i].id %>"><%= allplants[i].name  %>  </a></td>
                        <td scope="row"><%= allplants[i].category  %></td>
                      </tr>
                      <%} %> 
                    </tbody>
                  </table>
                <div class="card-footer"></div>
              </div>
            </div>
            </div>
            
          <!-- AI, Interaction -->
          
          <div class="col-xl-4 col-lg-3 mb-4" >
            <div class="card mb-4">
              <div style="margin: 5%;">
                <%if(result == null){%>
                  <h4>Please upload a picture to test</h4>
                <%}else{ %>
                        <h6 class="text-muted"><%= result.prediction.Plant %> </h6>
                        <p class="">Condition of Plant: <%= result.prediction.Condition %>, <br>confidence level: <%= (parseFloat(result.prediction.confidence_level)*100).toFixed(2) %>%</p>
               <%} %>
                <form autocomplete="off" action="/TestImage" Method="POST" enctype="multipart/form-data">
                    <label for="formFile" class="form-label" >Click "Choose File" to upload your picture</label>
                    <input class="form-control" type="file" name='file' id="formFile" >
                    <br/>
                  <button class="btn btn-primary" name="filename" type="submit">Submit</button>
                </form>
              </div>
              
            </div>
          </div>

          <!-- Modal Logout -->
          <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabelLogout"
            aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabelLogout">Ohh No!</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                  <p>Are you sure you want to Sign Out?</p>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Cancel</button>
                  <a href="login.html" class="btn btn-primary">SignOut</a>
                </div>
              </div>
            </div>
          </div>

        </div>
        <!---Container Fluid-->
      </div>
      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center my-auto">
            <span>copyright &copy; <script> document.write(new Date().getFullYear()); </script> 
            </span>
          </div>
        </div>
      </footer>
      <!-- Footer -->
    </div>
  </div>

